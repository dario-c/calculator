<%= form_tag("/get_price", method: "get") do %>
  <%= label_tag(:quantity, "Quantity:") %>
  <%= text_field_tag(:quantity) %>
   <%= label_tag(:price, "Price") %>
  <%= text_field_tag(:price) %>
  <%= label_tag(:country_code, "Country Code") %>
  <%= text_field_tag(:country_code) %>
  <%= submit_tag("Save") %>
<% end %>